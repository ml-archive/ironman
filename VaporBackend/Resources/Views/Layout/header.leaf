<!doctype html>
<html>
    <head>
        <title>Ironman</title>
        <link rel="stylesheet" media="screen" href="/css/app.css">
    </head>
    
    <body class="production fullbleed">
        <div class="layout vertical core-layout">
            <div class="core-layout__topbar">
                <section class="top-bar">
                    <div class="top-bar__left">
                        <button href="#()" class="btn btn-transparent core__left-sidebar-toggle">
                            <i class="fa fa-bars"></i>
                        </button>
                        #embed("Layout/Top-bar/logo")
                    </div>
                    <div class="top-bar__right">
                        #embed("Layout/Top-bar/actions-left")
